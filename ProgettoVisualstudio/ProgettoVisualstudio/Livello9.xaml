<UserControl x:Class="ProgettoVisualstudio.Livello9"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProgettoVisualstudio"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid x:Name="GridLivello"
      Width="250"
      Height="250"
      HorizontalAlignment="Center"
      VerticalAlignment="Center">

        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <!-- Riga 0 -->
        <Rectangle Grid.Row="0" Grid.Column="0" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="0" Grid.Column="1" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="0" Grid.Column="2" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="0" Grid.Column="3" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="0" Grid.Column="4" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="1" Grid.Column="0" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="0" Grid.Column="4" Fill="White" Stroke="Black" StrokeThickness="1.5"/>

        <!-- Riga 1 -->
        <Rectangle Grid.Row="1" Grid.Column="0" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="1" Grid.Column="1" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="1" Grid.Column="1" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="1" Grid.Column="2" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="1" Grid.Column="2" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="1" Grid.Column="3" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="1" Grid.Column="3" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="1" Grid.Column="4" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="1" Grid.Column="4" Fill="White" Stroke="Black" StrokeThickness="1.5"/>

        <!-- Riga 2 -->
        <Rectangle Grid.Row="2" Grid.Column="0" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="2" Grid.Column="0" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="2" Grid.Column="1" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="2" Grid.Column="1" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="2" Grid.Column="2" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="2" Grid.Column="2" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="2" Grid.Column="3" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="2" Grid.Column="3" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="2" Grid.Column="4" Fill="White" Stroke="Black" StrokeThickness="1.5"/>

        <!-- Riga 3 -->
        <Rectangle Grid.Row="2" Grid.Column="4" Fill="White" Stroke="Black" StrokeThickness="1.5"/>

        <!-- Riga 3 -->
        <Rectangle Grid.Row="3" Grid.Column="0" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="3" Grid.Column="0" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="3" Grid.Column="1" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="3" Grid.Column="1" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="3" Grid.Column="2" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="3" Grid.Column="2" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="3" Grid.Column="3" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="3" Grid.Column="3" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="3" Grid.Column="4" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="3" Grid.Column="4" Fill="White" Stroke="Black" StrokeThickness="1.5"/>

        <!-- Riga 4 -->
        <Rectangle Grid.Row="4" Grid.Column="0" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="4" Grid.Column="0" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="4" Grid.Column="1" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="4" Grid.Column="1" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="4" Grid.Column="2" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="4" Grid.Column="2" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="4" Grid.Column="3" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="4" Grid.Column="3" Fill="White" Stroke="Black" StrokeThickness="1.5"/>
        <Rectangle Grid.Row="4" Grid.Column="4" Fill="White" Stroke="Black" StrokeThickness="1.5"/>

    </Grid>



</UserControl>
